{
  "name": "xing-promise",
  "private": false,
  "version": "0.0.1",
  "description": "An ES6 Promise Interface that use's Angular's $q under the hood",
  "homepage": "https://github.com/LRDesign/xing-promise",
  "repository": {
    "type": "git",
    "url": "git://github.com/LRDesign/xing-promise.git"
  },
  "bugs": {
    "url": "https://github.com/LRDesign/xing-promise/issues"
  },
  "main": "dist/xing-promise.js",
  "dependencies": {
    "a1atscript": "^0.4.3"
  },
  "devDependencies": {
    "angular": "^1.3.15",
    "angular-mocks": "^1.3.15",
    "angular-new-router": "^0.5.3",
    "assert": "git+https://github.com/angular/assert#dist",
    "babel": "^5.4.4",
    "babelify": "^6.0.2",
    "gulp": "^3.8.9",
    "gulp-bump": "^0.2.2",
    "gulp-connect": "^2.0.6",
    "gulp-rename": "1.2.0",
    "karma": "^0.12.24",
    "karma-babel-preprocessor": "^5.1.0",
    "karma-browserify": "^4.1.2",
    "karma-chrome-launcher": "^0.1.5",
    "karma-cli": "0.0.4",
    "karma-commonjs": "0.0.13",
    "karma-firefox-launcher": "^0.1.4",
    "karma-jasmine": "^0.2.2",
    "karma-requirejs": "^0.2.2",
    "karma-traceur-preprocessor": "^0.4.0",
    "requirejs": "^2.1.15",
    "xing-traceur": "0.0.6"
  },
  "scripts": {
    "test": "gulp test:firefox; ./node_modules/.bin/karma start karma.conf.babel.js --browsers Firefox --single-run",
    "xing-traceur-amd": "xing-traceur --annotations --types --modules='amd' --dir src build/amd --maps moduleMaps.json; ./node_modules/.bin/xing-traceur --annotations --types --modules='amd' --dir ./node_modules/a1atscript/src build/amd --maps moduleMaps.json; ./node_modules/.bin/xing-traceur --annotations --types --modules='amd' --dir ./node_modules/xing-inflector/src build/amd --maps moduleMaps.json;",
    "babel-cjs": "babel src/xing-promise.js src --stage 0 --out-dir dist/cjs",
    "rjs": "r.js -o out=./dist/xing-promise.es5.js baseUrl=./build/amd name=xing-promise optimize=none",
    "amd": "npm run-script xing-traceur-amd; npm run-script rjs",
    "dist": "gulp dist; npm run-script amd; npm run-script babel-cjs;"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "stage": 0
        }
      ]
    ]
  },
  "jspm": {
    "main": "dist/cjs/xing-promise.js"
  },
  "author": {
    "name": "Hannah Howard",
    "email": "hannah@techgirlwonder.com"
  },
  "license": "Apache-2.0",
  "readme": "An ES6 Promise Interface that use's Angular's $q under the hood\n",
  "readmeFilename": "README.md",
  "_id": "xing-promise@0.0.1",
  "_shasum": "a6f7a8783df18040d185fbc61e08cd799c59703f",
  "_from": "https://registry.npmjs.org/xing-promise/-/xing-promise-0.0.1.tgz",
  "_resolved": "https://registry.npmjs.org/xing-promise/-/xing-promise-0.0.1.tgz"
}
