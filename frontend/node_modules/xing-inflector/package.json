{
  "name": "xing-inflector",
  "private": false,
  "version": "0.0.2",
  "description": "An Inflector for Xing",
  "homepage": "https://github.com/LRDesign/xing-inflector",
  "repository": {
    "type": "git",
    "url": "git://github.com/LRDesign/xing-inflector.git"
  },
  "bugs": {
    "url": "https://github.com/LRDesign/xing-inflector/issues"
  },
  "main": "dist/xing-inflector.js",
  "dependencies": {
    "a1atscript": "^0.4.0"
  },
  "devDependencies": {
    "angular": "^1.3.15",
    "angular-mocks": "^1.3.15",
    "angular-new-router": "^0.5.3",
    "assert": "git+https://github.com/angular/assert#dist",
    "babel": "^5.4.4",
    "babelify": "^6.0.2",
    "gulp": "^3.8.9",
    "gulp-bump": "^0.2.2",
    "gulp-connect": "^2.0.6",
    "gulp-rename": "^1.2.0",
    "gulp-traceur": "git+https://github.com/vojtajina/gulp-traceur#fix-sourcemaps",
    "karma": "^0.12.24",
    "karma-babel-preprocessor": "^5.1.0",
    "karma-browserify": "^4.1.2",
    "karma-chrome-launcher": "^0.1.5",
    "karma-cli": "0.0.4",
    "karma-commonjs": "0.0.13",
    "karma-firefox-launcher": "^0.1.4",
    "karma-jasmine": "^0.2.2",
    "karma-requirejs": "^0.2.2",
    "karma-traceur-preprocessor": "^0.4.0",
    "requirejs": "^2.1.15",
    "xing-traceur": "0.0.6"
  },
  "scripts": {
    "test": "karma start --browsers Firefox --single-run; ./node_modules/.bin/karma start karma.conf.babel.js --browsers Firefox --single-run",
    "xing-traceur-amd": "xing-traceur --annotations --types --modules='amd' --dir src build/amd --maps moduleMaps.json; ./node_modules/.bin/xing-traceur --annotations --types --modules='amd' --dir ./node_modules/a1atscript/src build/amd --maps moduleMaps.json",
    "babel-cjs": "babel src/xing-inflector.js src --stage 0 --out-dir dist/cjs",
    "rjs": "r.js -o out=./dist/xing-inflector.es5.js baseUrl=./build/amd name=xing-inflector optimize=none",
    "amd": "npm run-script xing-traceur-amd; npm run-script rjs",
    "dist": "gulp dist; npm run-script amd; npm run-script babel-cjs;"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "stage": 0
        }
      ]
    ]
  },
  "jspm": {
    "main": "dist/cjs/xing-inflector.js"
  },
  "author": {
    "name": "Hannah Howard",
    "email": "hannah@techgirlwonder.com"
  },
  "license": "Apache-2.0",
  "readme": "This is a simple inflector class used by the Xing Framework\n",
  "readmeFilename": "README.md",
  "_id": "xing-inflector@0.0.2",
  "_shasum": "ba2b2d8c693f9188c3e3357bdf8bad2160e9b5f4",
  "_from": "https://registry.npmjs.org/xing-inflector/-/xing-inflector-0.0.2.tgz",
  "_resolved": "https://registry.npmjs.org/xing-inflector/-/xing-inflector-0.0.2.tgz"
}
